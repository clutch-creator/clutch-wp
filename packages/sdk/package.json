{
  "name": "@clutch-wp/sdk",
  "version": "1.0.0",
  "description": "Clutch wordpress sdk",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "type": "module",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "swc src --out-dir dist --config-file ./.swcrc --delete-dir-on-start --strip-leading-paths",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-2.0",
  "dependencies": {
    "@clutch-wp/clutch-plugin": "workspace:*",
    "semver": "^7.6.3",
    "wp-types": "^4.68.0"
  },
  "devDependencies": {
    "@clutch-wp/eslint-config": "workspace:*",
    "@clutch-wp/prettier-config": "workspace:*",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.3.60",
    "@swc/helpers": "^0.5.17",
    "@types/semver": "^7.5.8",
    "eslint": "^9.20.0",
    "prettier": "^3.5.2",
    "typescript": "^5.6.0"
  }
}
